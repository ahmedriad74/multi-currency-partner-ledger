<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <!--
        Export XML Templates
    -->

    <template id="export_xml_template_base" name="Account Editable Report - Export XML Base Template">
<t t-translation="off">
<![CDATA[<?xml version="1.0" encoding="UTF-8"?>]]>
<DECLAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" t-att="['xsi:noNamespaceSchemaLocation', '%s%s%s.xsd' % (data['C_DOC'], data['C_DOC_SUB'], data['C_DOC_VER'])]">
    <DECLARHEAD>
        <TIN><t t-out="data['TIN']"/></TIN>
        <C_DOC><t t-out="data['C_DOC']"/></C_DOC>
        <C_DOC_SUB><t t-out="data['C_DOC_SUB']"/></C_DOC_SUB>
        <C_DOC_VER><t t-out="data['C_DOC_VER']"/></C_DOC_VER>
        <C_DOC_TYPE><t t-out="data['C_DOC_TYPE']"/></C_DOC_TYPE>
        <C_DOC_CNT><t t-out="data['C_DOC_CNT']"/></C_DOC_CNT>
        <C_REG><t t-out="data['C_REG']"/></C_REG>
        <C_RAJ><t t-out="data['C_RAJ']"/></C_RAJ>
        <PERIOD_MONTH><t t-out="data['PERIOD_MONTH']"/></PERIOD_MONTH>
        <PERIOD_TYPE><t t-out="data['PERIOD_TYPE']"/></PERIOD_TYPE>
        <PERIOD_YEAR><t t-out="data['PERIOD_YEAR']"/></PERIOD_YEAR>
        <C_STI_ORIG><t t-out="data['C_STI_ORIG']"/></C_STI_ORIG>
        <C_DOC_STAN><t t-out="data['C_DOC_STAN']"/></C_DOC_STAN>
        <LINKED_DOCS t-if="data['LINKED_DOCS'] is None" xsi:nil="true"/>
        <LINKED_DOCS t-else="">
            <DOC t-if="data['LINKED_DOCS']" t-foreach="data['LINKED_DOCS']" t-as="linked_doc" t-att="{'NUM': linked_doc_index + 1, 'TYPE': linked_doc['TYPE']}">
                <C_DOC><t t-out="linked_doc['C_DOC']"/></C_DOC>
                <C_DOC_SUB><t t-out="linked_doc['C_DOC_SUB']"/></C_DOC_SUB>
                <C_DOC_VER><t t-out="linked_doc['C_DOC_VER']"/></C_DOC_VER>
                <C_DOC_TYPE><t t-out="linked_doc['C_DOC_TYPE']"/></C_DOC_TYPE>
                <C_DOC_CNT><t t-out="linked_doc['C_DOC_CNT']"/></C_DOC_CNT>
                <C_DOC_STAN><t t-out="linked_doc['C_DOC_STAN']"/></C_DOC_STAN>
                <FILENAME><t t-out="linked_doc['FILENAME']"/></FILENAME>
            </DOC>
        </LINKED_DOCS>
        <D_FILL><t t-out="data['D_FILL']"/></D_FILL>
        <SOFTWARE><t t-out="data['SOFTWARE']"/></SOFTWARE>
    </DECLARHEAD>
    <DECLARBODY>
        <t t-out="0" />
    </DECLARBODY>
</DECLAR>
</t>
    </template>


    <template id="export_xml_template_account_base" name="Account Editable Report - Export XML Account Base Template">
        <t t-call="selferp_l10n_ua_ext.export_xml_template_base" t-translation="off">
            <t t-out="0" />

            <!-- FOOTER -->
            <HFILL><t t-out="data['HFILL']"/></HFILL>
            <HBOS><t t-out="data['HBOS']"/></HBOS>
            <HKBOS><t t-out="data['HKBOS']"/></HKBOS>
            <HBUH t-att="['xsi:nil', 'true' if data['HBUH'] is None else None]"><t t-out="data['HBUH']"/></HBUH>
            <HKBUH t-att="['xsi:nil', 'true' if data['HKBUH'] is None else None]"><t t-out="data['HKBUH']"/></HKBUH>
        </t>
    </template>


</odoo>
