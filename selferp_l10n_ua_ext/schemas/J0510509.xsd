				</xs:unique>
				<xs:unique name="UT1RXXXXG17S">
					<xs:selector xpath="T1RXXXXG17S"/>
					<xs:field xpath="@ROWNUM"/>
				</xs:unique>
				<xs:unique name="UT1RXXXXG18D">
					<xs:selector xpath="T1RXXXXG18D"/>
