				</xs:unique>
				<xs:unique name="UT1RXXXXG20">
					<xs:selector xpath="T1RXXXXG20"/>
					<xs:field xpath="@ROWNUM"/>
				</xs:unique>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DHead">
