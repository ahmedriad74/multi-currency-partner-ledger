			<xs:element name="HZY" type="DGYear"/>
			<xs:element name="HZM" type="DGMonth"/>
			<xs:element name="HZYP" type="DGYear" nillable="true" minOccurs="0"/>
			<xs:choice minOccurs="0">
				<xs:element name="HZKVP" type="DGKv"/>
				<xs:element name="HZMP" type="DGMonth"/>
			</xs:choice>
			<xs:element name="HNAME" type="DGHNAME"/>
			<xs:element name="HDDGV" type="DGDate" nillable="true" minOccurs="0"/>
			<xs:element name="HNDGV" type="xs:string" nillable="true" minOccurs="0"/>
			<xs:element name="HTIN" type="DGHTINJ"/>
			<xs:element name="HNPDV" type="DGHNPDV"/>
			<!--BODY-->
			<!--TABLE №1-->
			<xs:element name="T1RXXXXG3" type="MonthColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T1RXXXXG4" type="YearColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T1RXXXXG5" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T1RXXXXG6" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="R01G5" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R02G5" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R02G6" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<xs:element name="R03G2" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<!--TABLE №2-->
			<xs:element name="T2RXXXXG2" type="IndTaxNumColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T2RXXXXG3" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T2RXXXXG4D" type="DateColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T2RXXXXG5S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="R04G3" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<!--TABLE №2-->
			<xs:element name="T3RXXXXG2" type="IndTaxNumColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG3" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG4A" type="MonthColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG4B" type="YearColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG5" type="Decimal2Column" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG6D" type="DateColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="T3RXXXXG7S" type="StrColumn" nillable="true" minOccurs="0" maxOccurs="9999"/>
			<xs:element name="R05G5" type="DGdecimal0" nillable="true" minOccurs="0"/>
			<!--FOOTER-->
			<xs:element name="HFILL" type="DGDate"/>
			<xs:element name="HBOS" type="DGHBOS"/>
			<xs:element name="HKBOS" type="DGDRFO_10"/>
			<xs:element name="HBUH" type="DGHBUH" nillable="true" minOccurs="0"/>
			<xs:element name="HKBUH" type="DGDRFO_10" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
