<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">


    <record id="account_contract_sequence_purchase" model="ir.sequence">
        <field name="name">Purchase contracts</field>
        <field name="code">account.contract.sequence.purchase</field>
        <field name="prefix">PC/</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id"></field>
    </record>


    <record id="account_contract_sequence_sale" model="ir.sequence">
        <field name="name">Sale contracts</field>
        <field name="code">account.contract.sequence.sale</field>
        <field name="prefix">SC/</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id"></field>
    </record>


</odoo>
